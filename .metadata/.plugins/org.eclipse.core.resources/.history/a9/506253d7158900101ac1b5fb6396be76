package exemplos;

public class TesteConta {

	public static void main(String[] args) {
		//Criar novo objeto = instancia
		Conta conta1 = new Conta();
		conta1.numero = 123;
		conta1.titular = "Adriano";
		conta1.saldo = 1000.;
		
		conta1.deposito(100);
		
		if (conta1.saque(200)) {
			System.out.println("Saque efetuado!");
		}else {
			System.out.println("Saque n√£o efetuado.");
		}
		
		System.out.println("Saldo atual:"+conta1.saldo);
		
		
		conta1.saque(200);
		System.out.println("Saldo atual:"+conta1.saldo);
		
	}

}
