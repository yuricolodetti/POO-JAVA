package exemplos;

public class TesteAluno {

	public static void main(String[] args) {
		Aluno aluno1 = new Aluno();
		aluno1.matricula =123;
		aluno1.nome = "Jos√©";
		aluno1.email = "jose@gmail.com";
		
		Aluno aluno2 = new Aluno();
		aluno1.matricula =321;
		aluno1.nome = "Maria";
		aluno1.email = "maria@gmail.com";
		
		//Aluno aluno3 = aluno1;
		
		System.out.println(aluno1.matricula +"-"+ aluno1.nome +"-"+ aluno1.email);
		System.out.println(aluno2.matricula +"-"+ aluno2.nome +"-"+ aluno2.email);
		System.out.println(aluno1==aluno2);
		
	}

}
