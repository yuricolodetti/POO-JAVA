package exemplos;

public class Comparar {

	public static void main(String[] args) {
			int num1 = 10, num2 = 9;
			
			if (num1 > num2) {
				System.out.println("Num1 é maior.");
			} else if (num2 > num1) {
				System.out.println("Num 2 é maior");
			} else {
				System.out.println("Iguais");
			}
	}

}
